enum Priority {
   Priority1(".    "), Priority2("!    "), Priority3("!!!  "), Priority4("!!!!!");
   private String priority;

   Priority(String priority) {
       this.priority = priority;
   }

   public String getPriority() {
       return priority;
   }
//it's not  work
   public static Priority getPriority(String priority) {
       Priority pr = null;
       switch (priority) {
           case Priority1.getPriority(): //if case "." - it's work else error , that Priority1.getPriority() isn't constant
               pr = Priority1;
           case Priority2.getPriority():
               pr = Priority2;
           case Priority3.getPriority():
               pr = Priority3;
           case Priority4.getPriority():
               pr = Priority4;
       }

       return pr;
   }
   // it's work
   /*public static Priority getPriority(String priority) {
       Priority pr = null;
       if (priority == Priority1.getPriority())
           pr = Priority1;
       else if (priority == Priority2.getPriority())
           pr = Priority2;
       else if (priority == Priority3.getPriority())
           pr = Priority3;
       else
           pr = Priority4;
       return pr;
   }*/
}
